#compdef sbatch
_sbatch() {
	_arguments \
	  "(-p --partition)"{-p,--partition}"[partition]:partition:(experiment longexp short simulation)" \
	  "(-t --time)"{-t,--time}"[timelimit]:timelimit:" \
	  "--begin[delayed start]:delayed start:" \
	  "--nice[niceness]:niceness:" \
	  "--wrap[wrap]:wrap function:" \
	  "*::args:->command" && return 0
	# '*::command: _normal'
	# '*::args:->command' "$common[@]" && ret=0
	# shift 1 words
	# (( CURRENT-- ))
	_normal
	return
}
_sbatch "$@"
